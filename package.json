{
  "name": "agenda-de-cobro",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "electron": "^2.0.0-beta.7",
    "express": "^4.16.3",
    "font-awesome": "^4.7.0",
    "interact.js": "^1.2.8",
    "moment": "^2.21.0",
    "mongoose": "^5.0.11",
    "mongoose-deep-populate": "^3.1.1",
    "mongoose-float": "^1.0.2",
    "mongoose-unique-validator": "^2.0.0",
    "multer": "^1.3.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^5.0.0-beta.3"
  },
  "devDependencies": {
    "electron": "^1.8.4",
    "electron-packager": "^11.1.0",
    "foreman": "^2.0.0",
    "react-scripts": "1.1.1"
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "dev": "nf start",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=public/favicon.ico --prune=true --out=release-builds",
    "package-win": "electron-packager . agenda-de-cobro --overwrite --asar=true --platform=win32 --arch=ia32 --icon=public/favicon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Agenda de Cobro\"",
    "package-linux": "electron-packager . agenda-de-cobro --overwrite --asar=true --platform=linux --arch=x64 --icon=public/favicon.ico --prune=true --out=release-builds"
  }
}
